{
  "name": "$EXTENSION_NAME",
  "short_name": "$EXTENSION_NAME",
  "description": "$EXTENSION_DESCRIPTION",
  "version": "$EXTENSION_VERSION",
  "author": "$EXTENSION_AUTHOR",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "run_at": "document_start",
      "css": ["css/pure.css"],
      "js": [
        "js/content_scripts.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "key": ""
}
